@charset "UTF-8";
* {
  border: 0;
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  -webkit-text-size-adjust: none;
  -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
  -webkit-font-smoothing: antialiased;
  -webkit-overflow-scrolling: touch;
}

*:before, *:after {
  box-sizing: border-box;
}

body {
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  padding-right: constant(safe-area-inset-right);
  padding-left: constant(safe-area-inset-left);
  padding-right: env(safe-area-inset-right);
  padding-left: env(safe-area-inset-left);
  font-family: 'Noto Sans','NanumBarunGothic','돋움',Dotum,Arial,sans-serif;
  font-weight: 300;
  font-size: 14px;
  line-height: normal;
  word-break: keep-all;
  color: #666;
}

html {
  -webkit-text-size-adjust: none;
}

h1, h2, h3, h4, h5, h6 {
  line-height: normal;
  font-weight: normal;
  font-size: inherit;
}

ul, ol, li {
  list-style: none;
}

em, address {
  font-style: normal;
}

a {
  text-decoration: none;
  color: #666;
}

img {
  vertical-align: top;
  max-width: 100%;
  width: 100%;
}

table {
  border-spacing: 0;
  width: 100%;
  table-layout: fixed;
  border-collapse: collapse;
}

caption {
  position: absolute;
  left: -1000%;
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
  color: transparent;
}

input[type=text], input[type=password], select, textarea {
  -webkit-border-radius: 0;
  -webkit-appearance: none;
}

select::-ms-expand {
  display: none;
}

.ie9 select {
  padding-right: 0;
  background-image: none !important;
}

textarea {
  font-family: inherit;
  resize: none;
}

::selection {
  background: #92cfe7;
}

::placeholder {
  color: #bbb !important;
}

:-ms-input-placeholder {
  color: #bbb !important;
}

::-ms-input-placeholder {
  color: #bbb !important;
}

input[type="file"] {
  width: auto;
}

button {
  vertical-align: top;
  font-family: 'Noto Sans';
  background-color: transparent;
  cursor: pointer;
}

#skip_nav {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 999999999999;
}
#skip_nav > a {
  display: block;
  width: 1px;
  height: 1px;
  margin-bottom: -1px;
  text-align: center;
  color: #fff;
  overflow: hidden;
}
#skip_nav > a:focus, #skip_nav > a:active {
  width: 100%;
  height: auto;
  padding: 5px 0;
  background-color: #006cb8;
}

.footer_wrap {
  background: #efefef;
  margin-top: 50px;
}
.footer_wrap footer {
  padding: 19px;
  text-align: center;
}
.footer_wrap .link_box > ul {
  display: inline-block;
  margin-bottom: 8px;
}
.footer_wrap .link_box > ul:after {
  content: '';
  display: block;
  clear: both;
}
.footer_wrap .link_box > ul > li {
  display: inline-block;
  position: relative;
}
.footer_wrap .link_box > ul > li:not(:last-child) {
  margin-right: 16px;
}
.footer_wrap .link_box > ul > li:not(:last-child):after {
  content: '';
  display: block;
  position: absolute;
  background: #b8b8b8;
  width: 1px;
  height: calc(100% - 8px);
  right: -11px;
  top: 4px;
}
.footer_wrap .link_box > ul > li:nth-child(4):after {
  display: none;
}
.footer_wrap .link_box > ul > li > a {
  display: block;
  font-size: 12px;
}
.footer_wrap .link_box > ul > li > a:focus, .footer_wrap .link_box > ul > li > a:hover {
  color: #f26522;
}
.footer_wrap .link_box > ul .pc_content {
  display: none;
}
.footer_wrap .info_box {
  margin-bottom: 2px;
}
.footer_wrap .info_box > span {
  display: inline-block;
}
.footer_wrap .info_box > span:not(:last-child) {
  margin-right: 6px;
}
.footer_wrap .info_box .tel > strong {
  color: #005da4;
  font-size: 14px;
  font-weight: 500;
}

.lnb_wrap {
  display: none;
}

#ctn {
  padding: 90px 10px 0 10px;
  background: #fff;
}
#ctn .ctn_top {
  position: relative;
  padding-bottom: 13px;
  border-bottom: solid 1px #d9d9d9;
  margin-bottom: 27px;
}
#ctn .ctn_top .ctn_tit {
  margin-bottom: 6px;
  color: #222;
  font-size: 23px;
  font-weight: bold;
}

.location {
  width: calc(100% - 40px);
}
.location > ul {
  padding-left: 14px;
}
.location > ul:after {
  content: '';
  display: block;
  clear: both;
}
.location > ul > li {
  position: relative;
  float: left;
  line-height: 20px;
}
.location > ul > li:not(.home):before {
  content: '';
  position: absolute;
  top: 5px;
  left: -13px;
  width: 10px;
  height: 10px;
  background: url("../../images/ob-kr/ico_location_arr_m.png") no-repeat center/4px;
}
.location > ul > li:not(:last-child) {
  margin-right: 19px;
}
.location > ul > li.home {
  width: 12px;
  height: 19px;
  background: url("../../images/ob-kr/ico_home_m.png") no-repeat center/12px;
  margin-left: -14px;
}
.location > ul > li.on {
  color: #0092e0;
  font-weight: 500;
}
.location > ul > li.on ~ li {
  display: none;
}

.sns_box {
  position: absolute;
  right: 0;
  bottom: 17px;
}
.sns_box.open .sns {
  display: block;
  z-index: 10;
}
.sns_box.open .share {
  background: url("../../images/ob-kr/ico_btn_share_close.png") no-repeat center/30px;
}
.sns_box .share {
  background: url("../../images/ob-kr/ico_btn_share_open.png") no-repeat center/30px;
  display: block;
  width: 30px;
  height: 30px;
}
.sns_box .sns {
  display: none;
  position: absolute;
  width: 120px;
  height: 50px;
  top: -8px;
  right: 39px;
  padding: 10px;
  background: #f5f5f5;
  border: solid 1px #d7d7d7;
}
.sns_box .sns:after {
  content: '';
  display: block;
  clear: both;
}
.sns_box .sns:before {
  content: '';
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -5px;
  right: -5px;
  border-left: solid 1px #d7d7d7;
  border-top: solid 1px #d7d7d7;
  width: 7px;
  height: 7px;
  background: #f5f5f5;
  transform: rotate(135deg);
}
.sns_box .sns > li {
  float: left;
}
.sns_box .sns > li:not(:nth-last-child(-n + 4)) {
  margin-right: 5px;
}
.sns_box .sns > li > a {
  display: block;
  width: 29px;
  height: 29px;
}
.sns_box .sns .facebook {
  background: url("../../images/ob-kr/ico_sns_facebook_m.png") no-repeat center/29px;
}
.sns_box .sns .twitter {
  background: url("../../images/ob-kr/ico_sns_twitter_m.png") no-repeat center/29px;
}
.sns_box .sns .kakao {
  background: url("../../images/ob-kr/ico_sns_kakao_m.png") no-repeat center/29px;
}
.sns_box .sns .pdf {
  display: none;
}
.sns_box .sns .image {
  display: none;
}
.sns_box .sns .print {
  display: none;
}

@media screen and (min-width: 1217px) {
  .pc_fr {
    float: right;
  }

  body {
    font-size: 16px;
  }

  .footer_wrap footer {
    width: 1200px;
    margin: 0 auto;
    padding: 30px 0 30px 192px;
    text-align: left;
    background: url("../../images/ob-kr/f_logo.png") no-repeat center left 0;
  }
  .footer_wrap .link_box > ul > li:not(:last-child) {
    margin-right: 36px;
  }
  .footer_wrap .link_box > ul > li:not(:last-child):after {
    right: -18px;
    top: 5px;
  }
  .footer_wrap .link_box > ul > li:nth-child(4):after {
    display: block;
  }
  .footer_wrap .link_box > ul > li > a {
    font-size: 16px;
    color: #444;
  }
  .footer_wrap .link_box > ul .pc_content {
    display: inline-block;
  }
  .footer_wrap .info_box > span {
    font-size: 13px;
  }
  .footer_wrap .info_box > span:not(:last-child) {
    margin-right: 5px;
  }
  .footer_wrap .info_box .tel > strong {
    font-size: 20px;
  }
  .footer_wrap .copy {
    font-size: 13px;
  }

  #content {
    padding: 138px 0 0 0;
  }
  #content .content_inner {
    width: 1200px;
    margin: 0 auto;
  }
  #content .content_inner:after {
    content: '';
    display: block;
    clear: both;
  }
  #content .lnb_wrap {
    float: left;
    width: 250px;
    margin-right: 40px;
  }
  #content #ctn {
    float: left;
    width: calc(100% - 290px);
  }

  #ctn {
    padding: 46px 0 0 0;
  }
  #ctn .ctn_top {
    padding-bottom: 20px;
    margin-bottom: 45px;
  }
  #ctn .ctn_top .ctn_tit {
    margin-bottom: 12px;
    font-size: 32px;
  }

  .lnb_wrap {
    display: block;
  }
  .lnb_wrap #lnb .lnb_tit {
    background: url("../../images/ob-kr/bg_lnb_tit.gif") no-repeat center;
    display: table-cell;
    width: 250px;
    height: 140px;
    padding: 20px;
    color: #fff;
    font-size: 26px;
    font-weight: 500;
    vertical-align: middle;
    text-align: center;
  }
  .lnb_wrap #lnb > ul {
    border-bottom: solid 1px #666;
  }
  .lnb_wrap #lnb > ul > li.on > a {
    background: #0092e0;
    border-bottom: solid 3px #0075b3;
    color: #fff;
    font-weight: 500;
  }
  .lnb_wrap #lnb > ul > li.on > a:after {
    background-image: url("../../images/ob-kr/ico_lnb_minus.gif");
  }
  .lnb_wrap #lnb > ul > li.on:last-child > a {
    border-bottom: solid 3px #0075b3;
  }
  .lnb_wrap #lnb > ul > li:hover > a, .lnb_wrap #lnb > ul > li:focus > a {
    background: #0092e0;
    border-bottom: solid 3px #0075b3;
    color: #fff;
    font-weight: 500;
  }
  .lnb_wrap #lnb > ul > li:hover > a:after, .lnb_wrap #lnb > ul > li:focus > a:after {
    background-image: url("../../images/ob-kr/ico_lnb_minus.gif");
  }
  .lnb_wrap #lnb > ul > li:hover:last-child > a, .lnb_wrap #lnb > ul > li:focus:last-child > a {
    border-bottom: solid 3px #0075b3;
  }
  .lnb_wrap #lnb > ul > li:last-child > a {
    border: 0;
  }
  .lnb_wrap #lnb > ul > li > a {
    position: relative;
    display: table-cell;
    width: 100vw;
    padding: 15px 37px 15px 15px;
    height: 58px;
    border-bottom: solid 1px #d9d9d9;
    color: #444;
    font-size: 17px;
    vertical-align: middle;
    word-break: keep-all;
    transition: background .2s ease;
  }
  .lnb_wrap #lnb > ul > li > a:after {
    content: '';
    display: block;
    position: absolute;
    right: 15px;
    top: 50%;
    width: 12px;
    margin-top: -6px;
    height: 12px;
    background: url("../../images/ob-kr/ico_lnb_plus.gif") no-repeat center;
  }

  .location {
    width: 650px;
  }
  .location > ul {
    padding-left: 15px;
  }
  .location > ul > li {
    line-height: normal;
    font-size: 15px;
  }
  .location > ul > li:not(.home):before {
    top: 6px;
    left: -14px;
    background: url("../../images/ob-kr/ico_location_arr_pc.png") no-repeat center/auto;
  }
  .location > ul > li:not(:last-child) {
    margin-right: 24px;
  }
  .location > ul > li.home {
    width: 15px;
    height: 22px;
    background: url("../../images/ob-kr/ico_home_pc.png") no-repeat center/auto;
    margin-left: -15px;
  }

  .sns_box {
    bottom: 25px;
  }
  .sns_box .share {
    display: none;
  }
  .sns_box .sns {
    display: block;
    position: static;
    padding: 0;
    background: #fff;
    border: 0;
    width: auto;
    height: auto;
    top: auto;
    right: auto;
  }
  .sns_box .sns:after {
    content: '';
    display: block;
    clear: both;
  }
  .sns_box .sns:before {
    display: none;
  }
  .sns_box .sns > li {
    float: left;
  }
  .sns_box .sns > li:not(:last-child) {
    margin-bottom: 0;
    margin-right: 5px;
  }
  .sns_box .sns > li > a {
    width: 36px;
    height: 36px;
  }
  .sns_box .sns .facebook {
    background-image: url("../../images/ob-kr/ico_sns_facebook.png");
    background-size: auto;
  }
  .sns_box .sns .twitter {
    background-image: url("../../images/ob-kr/ico_sns_twitter.png");
    background-size: auto;
  }
  .sns_box .sns .kakao {
    background-image: url("../../images/ob-kr/ico_sns_kakao.png");
    background-size: auto;
  }
  .sns_box .sns .print {
    display: block;
    background: url("../../images/ob-kr/ico_sns_print.png") no-repeat center;
  }
  .sns_box .sns .image {
    display: block;
    background: url("../../images/ob-kr/ico_sns_image.png") no-repeat center;
  }
  .sns_box .sns .pdf {
    display: block;
    background: url("../../images/ob-kr/ico_sns_pdf.png") no-repeat center;
  }
}
@media screen and (min-width: 1217px) {
  :lang(en) .header_bottom .gnb_wrap {
    margin-left: 180px;
  }
  :lang(en) header .gnb_wrap #gnb > ul > li:not(:last-child) {
